    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@stylistic/eslint-plugin-ts": "^2.3.0",
    "@types/eslint__js": "^8.42.3",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.17.6",
    "@types/node": "^17.0.5",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
