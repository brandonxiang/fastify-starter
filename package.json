{
  "name": "fastify-starter",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run clean && APP_REGION=sg APP_ENV=dev tsup-node src/server.ts --watch --format esm --onSuccess \"node dist/server.js\"",
    "start": "node ./dist/server.js",
    "compile-schemas": "json2ts -i src/schemas -o src/types",
    "clean": "rimraf .tmp dist",
    "build": "tsup-node src/server.ts --format esm",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "10.0.1",
    "dayjs": "^1.11.13",
    "fastify": "5.0.0",
    "ioredis": "^5.4.1",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "^4.17.10",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "pnpm": {
    "overrides": {
      "rollup": "npm:@rollup/wasm-node"
    }
  }
}
