{
  "name": "fastify-starter",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run clean && APP_REGION=sg APP_ENV=dev tsup-node src/server.ts --watch --format esm --onSuccess \"node dist/server.js\"",
