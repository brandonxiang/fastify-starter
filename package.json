{
  "name": "fastify-starter",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run clean && APP_REGION=sg APP_ENV=dev tsup-node src/server.ts --watch --format esm --onSuccess \"node dist/server.js\"",
    "start": "node ./dist/server.js",
    "compile-schemas": "json2ts -i src/schemas -o src/types",
    "clean": "rimraf .tmp dist",
    "build": "tsup-node src/server.ts --format esm",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "fastify": "4.8.1",
    "@fastify/cors": "8.1.0",
    "ioredis": "^4.28.5",
    "knex": "^1.0.1",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "tsup": "^6.2.3",
    "@types/ioredis": "^4.28.8",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.5",
    "@types/uuid": "^9.0.8"
  }
